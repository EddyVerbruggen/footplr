{
  "name": "footplr",
  "version": "0.0.1",
  "description": "A native application built with NativeScript-Vue",
  "author": "Eddy Verbruggen <eddyverbruggen@gmail.com>",
  "license": "MIT",
  "nativescript": {
    "id": "com.footballplayerratings.app",
    "tns-ios": {
      "version": "6.5.2"
    },
    "tns-android": {
      "version": "6.5.3"
    }
  },
  "scripts": {
    "tsc": "cd app && tsc --skipLibCheck --watch",
    "ios-and-android": "tns run --bundle",
    "ios": "tns run ios --bundle",
    "ios.release": "tns build ios --bundle --release --forDevice --provision afdf502d-d9f3-49cd-9bec-438fe2d4295f",
    "ios.emulator": "tns run ios --bundle --emulator",
    "android": "tns run android --bundle",
    "android.release": "tns build android --bundle --release --for-device --key-store-path ./keystore-footplr.jks --key-store-password xs4all --key-store-alias footplr --key-store-alias-password xs4all"
  },
  "dependencies": {
    "nativescript-appversion": "~1.4.4",
    "nativescript-camera": "~4.5.0",
    "nativescript-feedback": "~1.5.0",
    "nativescript-imagepicker": "~7.1.0",
    "nativescript-iqkeyboardmanager": "~1.5.1",
    "nativescript-numeric-keyboard": "~4.3.1",
    "nativescript-pager": "~11.0.11",
    "nativescript-pdf-view": "~2.4.3",
    "nativescript-plugin-firebase": "~10.5.2",
    "nativescript-taptic-engine": "~2.1.0",
    "nativescript-theme-core": "~1.0.6",
    "nativescript-vue": "~2.4.0",
    "nativescript-web-image-cache": "git+https://github.com/EddyVerbruggen/nativescript-web-image-cache.git#2b7a51a36a289b39132daabca045e92784a26d70",
    "nativescript-youtubeplayer": "~3.0.1",
    "tns-core-modules": "~6.5.15"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "lazy": "1.0.11",
    "nativescript-dev-webpack": "~1.2.0",
    "nativescript-vue-template-compiler": "~2.4.0",
    "nativescript-worker-loader": "~0.9.5",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.1.0",
    "tns-platform-declarations": "~6.5.15",
    "typescript": "~3.4.5",
    "vue-loader": "^15.2.6",
    "webpack": "^4.16.4",
    "webpack-bundle-analyzer": "~3.6.0",
    "webpack-cli": "rm ^3.1.0"
  }
}
